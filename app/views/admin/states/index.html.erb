<%= link_to 'New State', new_admin_state_path %>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @states.each do |state| %>
      <tr id="state_<%= state.id %>">
        <td><%= state.name %></td>
        <td><%= link_to 'Edit', '#' %></td>
        <td><%= link_to 'Delete', '#' %></td>
        <td><%= link_to 'Make Default', make_default_admin_state_path(state), method: :post %></td>
      </tr>
    <% end %>
  </tbody>
</table>